{
  "name": "@emartech/collect-font-families",
  "description": "",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "test-watch": "ts-node node_modules/tape-watch/bin/tape-watch source/*.spec.ts source/**/*.spec.ts",
    "test": "ts-node node_modules/tape/bin/tape source/*.spec.ts source/**/*.spec.ts",
    "build": "rimraf dist && tsc",
    "semantic-release": "CI=true semantic-release pre && npm publish --access public && semantic-release post",
    "prepublish": "npm test && npm run build"
  },
  "config": {
    "commitTypeMap": {
      "chore": "patch"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emartech/font-family-collector"
  },
  "bugs": {
    "url": "https://github.com/emartech/font-family-collector"
  },
  "author": "Viktor Somodi <viktor.somodi@emarsys.com>",
  "license": "ISC",
  "homepage": "https://github.com/emartech/font-family-collector",
  "release": {
    "analyzeCommits": "@bubltechnology/customizable-commit-analyzer",
    "verifyConditions": {
      "path": "./node_modules/semantic-release/src/lib/plugin-noop.js"
    }
  },
  "devDependencies": {
    "@types/cheerio": "0.17.31",
    "@types/node": "6.0.45",
    "@types/ramda": "0.0.3",
    "@types/tape": "4.2.29",
    "tap-dot": "^1.0.5",
    "tape": "4.6.3",
    "tape-watch": "2.3.0",
    "ts-node": "3.0.2",
    "typescript": "2.3.1",
    "rimraf": "2.5.4",
    "semantic-release": "4.3.5"
  },
  "dependencies": {
    "cheerio": "0.22.0",
    "match-all": "1.2.2",
    "ramda": "0.23.0"
  }
}
